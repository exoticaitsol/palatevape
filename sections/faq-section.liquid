 <section
                class="about_page_banner bg_style"
                style="background-image: url('{{ section.settings.image | img_url:'master' }} ')">
                <div class="container">
                    <div class="about_un_txt">
                        <h2>{{ section.settings.head }}</h2>
                    </div>
                </div>
            </section>

            <section class="faq_accordian_page py_9">
                <div class="container">
                    <div class="accordian_faq_page_unde">
                        <div class="accordion accordion-flush"
                            id="accordionFlushExample">
                          
                          {% for block in section.blocks %}
                            <div class="accordion-item">
                                <h2 class="accordion-header"
                                    id="flush-heading{{ forloop.index }}">
                                    <button class="accordion-button collapsed"
                                        type="button" data-bs-toggle="collapse"
                                        data-bs-target="#flush-collapse{{ forloop.index }}"
                                        aria-expanded="false"
                                        aria-controls="flush-collapse{{ forloop.index }}">
                                       {{ block.settings.question }}
                                    </button>
                                </h2>
                                <div id="flush-collapse{{ forloop.index }}"
                                    class="accordion-collapse collapse"
                                    aria-labelledby="flush-heading{{ forloop.index }}"
                                    data-bs-parent="#accordionFlushExample">
                                    <div class="accordion-body">
                                         {{ block.settings.answer }}
                                    </div>
                                </div>
                            </div>
                          {% endfor %}
                           
                           
                        </div>
                    </div>
                </div>
            </section>

{% schema %}
  {
    "name": "Faq Page",
    "settings": [

         {
     "type": "image_picker",
      "id": "image",
        "label": "Add Image"
        },
       {
     "type": "text",
      "id": "head",
        "label": "Add heading"
        }  
    ],

     "blocks": [
  {
    "type": "Faqs",
    "name": "Faqs",
    "settings": [
      {
          "type": "text",
          "id": "question",
          "label": "Question"
        },
       {
          "type": "textarea",
          "id": "answer",
          "label": "Answer"
        }
    ]
  }

    ],
     "presets": [
    {
      "name": "FAQ Page"
   
    }
  ]
  }
{% endschema %}