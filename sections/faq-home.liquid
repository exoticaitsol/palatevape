 <section class="faq_sec py_9">
                <div class="container">
                    <div class="row">
                        <div class="col-md-5">
                            <div class="under_left_faq">
                                <div class="under_faq_txt">
                                    <span>{{ section.settings.head }}</span>
                                    <h2>{{ section.settings.title }}</h2>
                                    <div class="img_faq">
                                        <img src="{{ section.settings.img | img_url: 'original' }}" alt>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-7">
                            <div class="accordian_sec">
                                <div class="accordion accordion-flush"
                                    id="accordionFlushExample">

                                    {% for block in section.blocks %}
                                    <div class="accordion-item">
                                        <h2 class="accordion-header"
                                            id="flush-heading{{ forloop.index }}">
                                            <button
                                                class="accordion-button collapsed"
                                                type="button"
                                                data-bs-toggle="collapse"
                                                data-bs-target="#flush-collapse{{ forloop.index }}"
                                                aria-expanded="false"
                                                aria-controls="flush-collapse{{ forloop.index }}">
                                                {{ block.settings.question }}
                                            </button>
                                        </h2>
                                        <div id="flush-collapse{{ forloop.index }}"
                                            class="accordion-collapse collapse"
                                            aria-labelledby="flush-heading{{ forloop.index }}"
                                            data-bs-parent="#accordionFlushExample"
                                            style>
                                            <div class="accordion-body">
                                               {{ block.settings.answer }}
                                            </div>
                                        </div>
                                    </div>
                                           {% endfor %}
                                  
                                   
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

{% schema %}
{
  "name": "FAQ Home",
  "tag": "section",

  "settings": [
     {
        "type": "text",
        "id": "head",
        "label":"Add  Heading"
      },
    {
        "type": "text",
        "id": "title",
        "label":"Add  Title"
      },
    {
        "type": "image_picker",
        "id": "img",
        "label":"Add to Image"
      } 
 
  ],
   "blocks": [
  {
    "type": "Collapsible-row",
    "name": "Collapsible row",
    "settings": [
      {
          "type": "text",
          "id": "question",
          "label": "Heading"
        },
       {
          "type": "text",
          "id": "answer",
          "label": "Row content"
        }
    ]
  }

    ],
  "presets": [
    {
      "name": "FAQ Home"
   
    }
  ]
}
{% endschema %}